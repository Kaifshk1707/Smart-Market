{
  "expo": {
    "name": "Smart Market",
    "slug": "Smart-Market",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/transLogo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/transLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
