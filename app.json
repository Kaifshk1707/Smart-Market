        "foregroundImage": "./assets/AppLogo2.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kaifshk17.SmartMarket"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/AppLogo2.png",
          "imageWidth": 200
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "fb08c343-deb3-4c9e-a870-fb69cbb7c847"
      }
    },
    "owner": "kaifshk17"
